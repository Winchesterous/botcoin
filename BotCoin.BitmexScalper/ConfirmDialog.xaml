<Window x:Class="BotCoin.BitmexScalper.ConfirmDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BotCoin.BitmexScalper" WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow"
        mc:Ignorable="d" ResizeMode="NoResize" Title="Confirm Stop-Order Cancelation" Height="84.99" Width="273.996" FontSize="16">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Button Content="YES" Margin="10" Background="Green" Foreground="White" Click="OnYesClick"/>
        <Button Content="NO" Margin="10" Background="LightCoral" Foreground="White" Grid.Column="1" Click="OnNoClick"/>
    </Grid>
</Window>
